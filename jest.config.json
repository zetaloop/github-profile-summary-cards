{
    "collectCoverage": true,
    "clearMocks": true,
    "moduleFileExtensions": ["js", "ts"],
    "transform": {
        "^.+\\.ts$": "ts-jest"
    },
    "moduleNameMapper": {
        "^axios$": "<rootDir>/node_modules/axios/dist/node/axios.cjs",
        "^retry-axios$": "<rootDir>/node_modules/retry-axios/dist/index.js"
    },
    "setupFilesAfterEnv": [],
    "testMatch": ["**/*.test.ts"],
    "verbose": true,
    "collectCoverageFrom": ["**/*.{ts,jx}", "!**/node_modules/**", "!**/dist/**, !**/lib/**"]
}
